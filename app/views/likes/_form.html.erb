<%= simple_form_for(@like) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :photo_id %>
    <%= f.input :user_id %>
    <%= f.association :user %>
    <%= f.association :photo %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
